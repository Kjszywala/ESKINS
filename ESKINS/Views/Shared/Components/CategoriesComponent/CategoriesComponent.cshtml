@model List<ESKINS.DbServices.Models.CategoriesModels>
@section head{
}
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<div class="cd-filter-block" id="searchCategories">
	<h4>Category</h4>
	<ul class="cd-filter-content cd-filters list" style="display:none;">
		@foreach(var item in Model)
		{
			<li>
                <input class="category-checkbox" type="checkbox" id="@item.CategoryDescription" name="@item.CategoryDescription" value="@item.CategoryDescription">
				<label class="checkbox-label" for="@item.CategoryDescription">@item.CategoryDescription</label>
			</li>
		}
	</ul> <!-- cd-filter-content -->
</div> <!-- cd-filter-block -->

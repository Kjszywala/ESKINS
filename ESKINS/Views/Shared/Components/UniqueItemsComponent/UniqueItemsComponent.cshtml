@model List<ESKINS.DbServices.Models.CMS.Qualities>
<div class="cd-filter-block">
	<h4>Unique Items</h4>

	<ul class="cd-filter-content cd-filters list" style="display:none;">
		@foreach(var item in Model)
		{
			<li>
				<input class="unique-checkbox" type="checkbox" id="@item.Quality" name="@item.Quality" value="@item.Quality">
				<label class="checkbox-label" for="@item.Quality">@item.Quality</label>
			</li>
		}
	</ul> <!-- cd-filter-content -->
</div> <!-- cd-filter-block -->
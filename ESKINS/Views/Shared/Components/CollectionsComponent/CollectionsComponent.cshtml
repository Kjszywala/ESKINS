@model List<ESKINS.DbServices.Models.ItemCollectionsModels>
<div class="cd-filter-block">
	<h4>Collection</h4>

	<ul class="cd-filter-content cd-filters list" style="display:none;">
		@foreach(var item in Model)
		{
			<li>
				<input type="radio" name="@item.ItemCollection" id="@item.ItemCollection" checked>
				<label class="radio-label" for="@item.ItemCollection">@item.ItemCollection</label>
			</li>
		}
	</ul> <!-- cd-filter-content -->
</div> <!-- cd-filter-block -->
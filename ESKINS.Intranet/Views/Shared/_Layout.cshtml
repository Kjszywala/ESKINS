<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ESKINS.Intranet</title>
    @RenderSection("head", required: false)
    <link rel="stylesheet" href="~/css/reset.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/layout.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ESKINS.Intranet.styles.css" asp-append-version="true" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
</head>
<body style="background-color: #222222">
    <div id="leftDiv" style="float: left;">
        <div class="area">
        </div>
        <nav class="main-menu">
            <ul>
                <li class="has-subnav">
                    <a href="@Url.Action("Index", "Customers")">
                        <i class="fa fa-user fa-2x"></i>
                        <span class="nav-text">
                            Customers
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Invoices")">
                        <i class="fa fa-envelope fa-2x"></i>
                        <span class="nav-text">
                            Invoices
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Items")">
                        <i class="fa fa-list fa-2x"></i>
                        <span class="nav-text">
                            Items
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Orders")">
                        <i class="fa fa-truck fa-2x"></i>
                        <span class="nav-text">
                            Orders
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Sellers")">
                        <i class="fa fa-bullhorn fa-2x"></i>
                        <span class="nav-text">
                            Sellers
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Users")">
                        <i class="fa fa-users fa-2x"></i>
                        <span class="nav-text">
                            Users
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Targets")">
                        <i class="fa fa-bullseye fa-2x"></i>
                        <span class="nav-text">
                            Targets
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "ItemPriceHistory")">
                        <i class="fa fa-archive fa-2x"></i>
                        <span class="nav-text">
                            Item Price Histories
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "ItemLogs")">
                        <i class="fa fa-info-circle fa-2x"></i>
                        <span class="nav-text">
                            Item Logs
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "ItemQualities")">
                        <i class="fa fa-star fa-2x"></i>
                        <span class="nav-text">
                            Item Qualities
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Phases")">
                        <i class="fa fa-cog fa-2x"></i>
                        <span class="nav-text">
                            Item Phases
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "ItemLocations")">
                        <i class="fa fa-map-marker fa-2x"></i>
                        <span class="nav-text">
                            Item Locations
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "ItemCollections")">
                        <i class="fa fa-folder fa-2x"></i>
                        <span class="nav-text">
                            Item Collections
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "ItemExteriors")">
                        <i class="fa fa-globe fa-2x"></i>
                        <span class="nav-text">
                            Item Exteriors
                        </span>
                    </a>
                </li>
                <li class="has-subnav">
                    <a href="@Url.Action("Index", "Categories")">
                        <i class="fa fa-briefcase fa-2x"></i>
                        <span class="nav-text">
                            Item Categories
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "PaymentMethods")">
                        <i class="fa fa-money fa-2x"></i>
                        <span class="nav-text">
                            Fx Rates
                        </span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "PaymentMethods")">
                        <i class="fa fa-dollar fa-2x"></i>
                        <span class="nav-text">
                            Payment Methods
                        </span>
                    </a>
                </li>
                <li class="has-subnav">
                    <a href="@Url.Action("Index", "ErrorLogs")">
                        <i class="fa fa-bug fa-2x"></i>
                        <span class="nav-text">
                            Error Log
                        </span>
                    </a>
                </li>
            </ul>
        </nav>
    </div>
    <div id="rightDiv">
        <header>
            <link href="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
            <script src="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>
            <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

            <nav class="navbar navbar-icon-top navbar-expand-lg navbar-dark">
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav mr-auto">
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </nav>
        </header>
    </div>
    <br />
    <div class="container" style="margin-top: auto; min-height:685px; width: 1200px; margin-right:auto; margin-left: auto;">
            @RenderBody()
    </div>
    <br />
    <div class="footer-clean " id="footer1" style="background: #121212; color: white;">
        <footer>
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-sm-4 col-md-3 item">
                        <h3>Services</h3>
                        <ul>
                            <li><a href="#">Web design</a></li>
                            <li><a href="#">Development</a></li>
                            <li><a href="#">Hosting</a></li>
                        </ul>
                    </div>
                    <div class="col-sm-4 col-md-3 item">
                        <h3>About</h3>
                        <ul>
                            <li><a href="#">Company</a></li>
                            <li><a href="#">Team</a></li>
                            <li><a href="#">Legacy</a></li>
                        </ul>
                    </div>
                    <div class="col-sm-4 col-md-3 item">
                        <h3>Careers</h3>
                        <ul>
                            <li><a href="#">Job openings</a></li>
                            <li><a href="#">Employee success</a></li>
                            <li><a href="#">Benefits</a></li>
                        </ul>
                    </div>
                    <div class="col-lg-3 item social">
                        <a href="#"><i class="icon ion-social-facebook"></i></a><a href="#"><i class="icon ion-social-twitter"></i></a><a href="#"><i class="icon ion-social-snapchat"></i></a><a href="#"><i class="icon ion-social-instagram"></i></a>
                        <p class="copyright">E-SKINS - &copy; @DateTime.Now.Year</p>
                    </div>
                </div>
            </div>
        </footer>
    </div>
   @* <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - ESKINS.Intranet - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>*@
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
